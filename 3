//3 
#include <iostream>
#include <string>
using namespace std;

void main()
{
  string str = "<([])>()[]<<()>>";
	bool   correct = false;
	int    cnt1 = 0;
	int    cnt2 = 0;
	int    cnt3 = 0;

	for(int i = 0; i < str.length(); i++)
	{
		switch (str[i])
		{
		case '<': cnt1++; break;
		case '>': cnt1--; break;
		case '(': cnt2++; break;
		case ')': cnt2--; break;
		case '[': cnt3++; break;
		case ']': cnt3--; break;
		default: break;
		}
		if(cnt1 < 0 || cnt2 < 0 || cnt3 < 0)
			break;
	}
	if(cnt1 == 0 && cnt2 == 0 && cnt3 == 0)
		correct = true;

	if(correct) 
		cout << "CORRECT\n";
	else
		cout << "UNCORRECT\n";

	system("pause");
}
